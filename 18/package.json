{
  "name": "integration-service",
  "version": "1.0.0",
  "description": "Integration Layer",
  "source": "src/index.js",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --silent",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test-verbose": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "start": "node --experimental-modules --es-module-specifier-resolution=node src/index",
    "dev": "nodemon --experimental-modules --es-module-specifier-resolution=node src/index",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "db:config": "node --experimental-modules --es-module-specifier-resolution=node setup/config-setup",
    "file": "node --experimental-modules --es-module-specifier-resolution=node",
    "create:schema": "node --experimental-modules --es-module-specifier-resolution=node setup/joi-schema-generator",
    "watch": "parcel watch",
    "build": "parcel build",
    "serve": "node --enable-source-maps --es-module-specifier-resolution=node dist/index",
    "format": "prettier-eslint --write \"**/*.js\" && prettier-eslint --write \"**/*.json\"",
    "prepare": "husky install",
    "debug": "nodemon --experimental-modules --es-module-specifier-resolution=node --trace_gc --max-old-space-size=100 --inspect src/index"
  },
  "author": "",
